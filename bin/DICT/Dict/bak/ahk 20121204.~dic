[常用命令]
InputBox=InputBox, OutputVar [, Title, Prompt, HIDE, Width, Height, X, Y, Font, Timeout, Default]|InputBox, OutputVar , Title, Prompt, , 260, 120|InputBox, OutputVar , Title, Prompt, 260, 120, , , , , Default
StringReplace=StringReplace, OutputVar, InputVar, SearchText [, ReplaceText, ReplaceAll?]|StringReplace, clipboard, clipboard, `r`n, , All
SplitPath=SplitPath, InputVar [, OutFileName, OutDir, OutExtension, OutNameNoExt, OutDrive]|SplitPath, FullFileName, name|SplitPath, FullFileName,, dir|SplitPath, FullFileName, name, dir, ext, name_no_ext, drive
SplitPath all=SplitPath, FullFileName, name, dir, ext, name_no_ext, drive
SplitPath dir=SplitPath, FullFileName,, dir
SplitPath ext=SplitPath, FullFileName, , ,ext
